{
    "comments": {
        "blockComment": [
            "/*",
            "*/"
        ],
        "lineComment": "//"
    },
    "brackets": [
        [ "(", ")" ],
        [ "[", "]" ],
        [ "{", "}" ],
        [ "\\", "\\" ]
    ],
    "autoClosingPairs": [
        [ "(", ")" ],
        [ "[", "]" ],
        [ "{", "}" ],
        [ "'", "'" ],
        [ "\"", "\"" ],
        [ "\\", "\\" ]
    ],
    "autoCloseBefore": ";:.,=}]) \n\t",
    "surroundingPairs": [
        [ "(", ")" ],
        [ "[", "]" ],
        [ "{", "}" ],
        [ "'", "'" ],
        [ "\"", "\"" ],
        [ "\\", "\\" ]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^(.)*{\\s*",
        "decreaseIndentPattern": "^(\\s)*}(\\s)*"
    }
}