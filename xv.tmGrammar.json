{
    "scopeName": "source.xv",
    "patterns": [
        { "include": "#numeric" },
        { "include": "#string" },
        { "include": "#comment" },
        { "include": "#comment_large" },
        { "include": "#punct" },
        { "include": "#punct2" },
        { "include": "#punct3" },
        { "include": "#ident" }
    ],
    "repository": {
        "numeric": {
            "name": "constant.numeric",
            "match": "\\d(\\w|\\.|(e\\+)|(E\\+)|(e\\-)|(E\\-))*"
        },
        "string": {
            "name": "string",
            "match": "\"((\\\\\")|[^\"])*\""
        },
        "comment": {
            "name": "comment",
            "match": "//.*\\n"
        },
        "comment_large": {
            "name": "comment",
            "begin": "\\/\\*",
            "end": "\\*\\/"
        },
        "punct": {
            "name": "keyword.operator",
            "match": "[@\\^~:\\?#%&\\*\\+\\|\\/=<>!-]+"
        },
        "punct2": {
            "name": "punctuation.separator",
            "match": "[\\[\\]\\(\\)\\{\\};,\\.]"
        },
        "punct3": {
            "name": "punctuation.separator",
            "match": "[\\$\\\\]"
        },
        "ident": {
            "name": "entity",
            "match": "[A-Za-z_]\\w*",
            "captures": [{ "patterns": [
                { "name": "keyword", "match": "\\bsic\\b" },
                { "name": "keyword", "match": "\\bnon\\b" },
                { "name": "keyword", "match": "\\bspatium\\b" },
                { "name": "keyword", "match": "\\bgenus\\b" },
